{
  "name": "securefacegateway-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/index.js",
  "author": {
    "name": "LianQi",
    "url": "https://lianqidev.cn"
  },
  "description": "Face recognition access control system based on Vue 3 and Electron for graduation design",
  "repository": {
    "type": "git",
    "url": "https://github.com/LianQi-Kevin/SecureFaceGateway_FrontEnd"
  },
  "license": "APACHE-2.0",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "tsc --noEmit && vite build",
    "vite:preview": "vite preview",
    "electron:start": "electron --trace-warnings .",
    "electron:build": "tsc --noEmit && vite build && electron-builder",
    "electron:postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "securefacegateway-frontend",
    "appId": "cn.lianqidev.securefacegateway-frontend",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        "portable"
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "compression": "maximum",
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "!node_modules"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^10.9.0",
    "@vueuse/integrations": "^10.9.0",
    "axios": "^1.6.8",
    "element-plus": "^2.5.1",
    "jwt-decode": "^3.1.2",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "sass": "^1.70.0",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-inspect": "^0.8.1"
  }
}
